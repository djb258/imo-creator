{
  "drawio_integration": {
    "version": "1.0.0",
    "auto_generation": true,
    "diagram_types": [
      {
        "name": "repository-architecture",
        "description": "High-level repository structure and data flow",
        "template": "architecture",
        "auto_update": true,
        "triggers": ["push", "gitingest_update"]
      },
      {
        "name": "mcp-routing",
        "description": "MCP tool routing and fallback mechanisms",
        "template": "network",
        "auto_update": true,
        "triggers": ["config_change", "mcp_registry_update"]
      },
      {
        "name": "cicd-workflow",
        "description": "GitHub Actions workflow visualization",
        "template": "process",
        "auto_update": true,
        "triggers": ["workflow_change", "branch_update"]
      }
    ],
    "gitingest_config": {
      "include_patterns": [
        "*.py",
        "*.js",
        "*.ts",
        "*.md",
        "*.yml",
        "*.yaml",
        "*.json",
        "*.drawio"
      ],
      "exclude_patterns": [
        "node_modules/*",
        "*.log",
        ".env*",
        "*.tmp"
      ],
      "max_file_size": 10485760,
      "output_format": "structured"
    },
    "diagram_metadata": {
      "composio_endpoint": "https://composio-imo-creator-url.onrender.com",
      "frontend_url": "https://imo-creator.vercel.app",
      "api_key": "ak_t-F0AbvfZHUZSUrqAGNn",
      "doctrine_branches": [
        "actions-workflows",
        "ai-human-readable",
        "drawio-ingest"
      ]
    },
    "vs_code_integration": {
      "extension": "hediet.vscode-drawio",
      "auto_preview": true,
      "export_formats": ["png", "svg"],
      "theme": "auto"
    }
  }
}