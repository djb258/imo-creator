{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["glossary"],
  "properties": {
    "glossary": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["acronym", "stands_for", "definition"],
        "properties": {
          "acronym": { "type": "string" },
          "stands_for": { "type": "string" },
          "definition": { "type": "string" }
        }
      }
    }
  }
}


