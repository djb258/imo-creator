{
  "role": "VAULT_DB",
  "current_driver": "neon",
  "description": "Production data vault with schema enforcement, audit logs, and validated record storage",
  "altitude": "5k",
  "interface_contract": {
    "input": "validated_record (object passing gatekeeper, includes unique_id, schema_version)",
    "output": "vault_confirmation (object with record_id, vault_timestamp, audit_log_id)"
  },
  "capabilities": [
    "schema_enforcement",
    "audit_logging",
    "immutable_records",
    "sql_queries"
  ],
  "status_endpoint": "/manual/troubleshooting/system_diagnostics.json#vault_db",
  "ort_manual": "/manual/ort-manuals/vault_db.ort.md",
  "supported_drivers": [
    {
      "name": "neon",
      "version": "latest",
      "provider": "Neon Serverless Postgres",
      "api_endpoint": "postgresql://user@endpoint.neon.tech"
    },
    {
      "name": "supabase",
      "version": "latest",
      "provider": "Supabase PostgreSQL",
      "api_endpoint": "postgresql://postgres@db.supabase.co"
    },
    {
      "name": "planetscale",
      "version": "latest",
      "provider": "PlanetScale MySQL",
      "api_endpoint": "mysql://user@endpoint.psdb.cloud"
    }
  ],
  "barton_doctrine_compliance": {
    "requires_composio": false,
    "heir_orbt_tracking": true,
    "gatekeeper_validation": true,
    "direct_access_forbidden": true,
    "validator_required": true
  },
  "health_check": {
    "method": "SELECT",
    "endpoint": "/validator/health",
    "expected_latency_ms": 150
  },
  "access_policy": {
    "direct_writes": "forbidden",
    "validator_layer": "required",
    "gatekeeper_approval": "mandatory"
  }
}
