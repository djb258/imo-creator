{
  "platform": "nextjs",
  "code": {
    "lang": "ts",
    "scheme": "blackbox",
    "reactRuntime": "classic"
  },
  "style": {
    "scheme": "css-modules",
    "defaultStyleCssFilePath": "src/app/globals.css"
  },
  "images": {
    "scheme": "public-files",
    "publicDir": "../public",
    "publicUrlPrefix": "/"
  },
  "tokens": {
    "scheme": "theo",
    "tokensFilePath": "plasmic-tokens.theo.json"
  },
  "srcDir": "src",
  "defaultPlasmicDir": "src/components/plasmic",
  "projects": [],
  "globalVariants": {
    "variantGroups": [
      {
        "id": "Screen",
        "name": "Screen",
        "projectId": "*",
        "contextFilePath": "src/lib/plasmic-context.tsx"
      }
    ]
  },
  "cliVersion": "0.1.342",
  "$schema": "https://unpkg.com/@plasmicapp/cli@0.1.342/dist/plasmic.schema.json"
}
