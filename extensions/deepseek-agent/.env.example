# DeepSeek API Configuration
# Copy this file to .env and fill in your actual values

# Required: Your DeepSeek API key
# Get your API key from: https://platform.deepseek.com/api_keys
DEEPSEEK_API_KEY=your_api_key_here

# Optional: API endpoint (default: https://api.deepseek.com/v1)
DEEPSEEK_API_ENDPOINT=https://api.deepseek.com/v1

# Optional: Model to use (default: deepseek-chat)
# Options: deepseek-chat, deepseek-coder
DEEPSEEK_MODEL=deepseek-chat

# Optional: Temperature for response generation (0-2, default: 0.7)
DEEPSEEK_TEMPERATURE=0.7

# Optional: Maximum tokens for response (default: 4096)
DEEPSEEK_MAX_TOKENS=4096

# Optional: Enable MCP server (default: true)
DEEPSEEK_ENABLE_MCP=true

# Optional: MCP server port (default: 7002)
DEEPSEEK_MCP_PORT=7002

# Optional: Context window size in lines (default: 10)
DEEPSEEK_CONTEXT_WINDOW=10
