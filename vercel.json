{
  "version": 2,
  "redirects": [
    { "source": "/", "destination": "/docs/blueprints/ui/overview.html" },
    { "source": "/ui", "destination": "/docs/blueprints/ui/overview.html" },
    { "source": "/overview", "destination": "/docs/blueprints/ui/overview.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "X-Powered-By", "value": "IMO-Creator" }
      ]
    }
  ]
}