{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/",          "destination": "/docs/blueprints/ui/overview.html" },
    { "source": "/overview",  "destination": "/docs/blueprints/ui/overview.html" },
    { "source": "/input",     "destination": "/docs/blueprints/ui/input.html" },
    { "source": "/middle",    "destination": "/docs/blueprints/ui/middle.html" },
    { "source": "/output",    "destination": "/docs/blueprints/ui/output.html" }
  ],
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    }
  }
}